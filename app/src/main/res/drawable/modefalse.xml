<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:andoid="http://schemas.android.com/aapt">

    <item>
        <shape
            android:shape = "oval">
            <solid
                andoid:color = "@color/white_rice"/>
            <size
                android:height="8dp"
                android:width="8dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/purple_2"/>
        </shape>
    </item>

    <item
        android:drawable="@drawable/baseline_light_mode_24"
        android:top="8dp"
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"/>

</layer-list>